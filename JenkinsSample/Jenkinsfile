pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
            echo 'Hello user' 
            }
        }
        stage('Test'){
        	steps{
        	echo 'Testing..'
        	}
        }
        stage('Deploy'){
        	steps{
        	echo 'Deploying..'
        	}
        }
        stage('Jarfile'){
        	steps{
	        	echo 'jar file generated'
	        	 dir ('build') {
	        	 	bat 'ant build.xml'
                }
        	}
        }
        
     }
}